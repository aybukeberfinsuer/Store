@model IEnumerable<Category>
        <div my-4>
                <ul class="list-group my-4">
                        @foreach(var item in Model){
                                <li class="list-group-item">
                                        <a asp-action="Index" asp-route-categoryId="@item.CategoryId">
                                                @item.CategoryName
                                         </a>                                        
                                </li>
                        }
                </ul>
        </div>